    (emit "movl  %eax,     ~s(%rsp)" si)
    (emit-expr (- si wordsize) env expr2)
    (emit "sarl  $~s,      %eax" int_tag_width)
    (emit "imull ~s(%rsp), %eax" si)
    (emit "shll  $~s,      %eax" int_tag_width)
    (emit "orl   $~s,      %eax" int_tag)
)

