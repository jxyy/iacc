(load "compiler.scm")

;; native unary primitives
(define-primitive (fxadd1 si env expr)
    (emit-expr si env expr)
    (emit "addl $~s, %eax" (emit-immediate 1))
)

(define-primitive (fxsub1 si env expr)
    (emit-expr si env expr)
    (emit "subl $~s, %eax" (emit-immediate 1))
)

(define-primitive (char->fixnum si env expr) 
    (emit-expr si env expr)
    (emit "shrl $6,  %eax")
    (emit "orl  $~s, %eax" int_tag)
)

(define-primitive (fixnum->char si env expr) 
    (emit-expr si env expr)
    (emit "shll $6,  %eax")
    (emit "orl  $~s, %eax" char_tag)
)

(define-primitive (fixnum? si env expr) 
    (emit-expr si env expr)
    (emit "andl   $~s,  %eax" (mask int_tag_width))
    (emit "cmpl   $~s,  %eax" int_tag)
    (emit "sete   %al")
    (emit "shll   $~s,  %eax" bool_tag_width)
    (emit "orl    $~s,  %eax" bool_tag)
)

(define-primitive (boolean? si env expr) 
    (emit-expr si env expr)
    (emit "andl   $~s,  %eax" (mask bool_tag_width))
    (emit "cmpl   $~s,  %eax" bool_tag)
    (emit "sete   %al")
    (emit "shll   $~s,  %eax" bool_tag_width)
    (emit "orl    $~s,  %eax" bool_tag)
)

(define-primitive (char? si env expr) 
    (emit-expr si env expr)
    (emit "andl   $~s,  %eax" (mask char_tag_width))
    (emit "cmpl   $~s,  %eax" char_tag)
    (emit "sete   %al")
    (emit "shll   $~s,  %eax" bool_tag_width)
    (emit "orl    $~s,  %eax" bool_tag)
)

(define-primitive (fxzero? si env expr)
    (emit-expr si env expr)
    (emit "cmpl   $0,   %eax")
    (emit "sete   %dl")
    (emit "xorl   %eax, %eax")
    (emit "mov    %dl,  %al")
    (emit "shll   $~s,  %eax" bool_tag_width)
    (emit "orl    $~s,  %eax" bool_tag)
)

(define-primitive (not si env expr)
    (emit-expr si env expr)
    (emit "cmpl   $47,   %eax") ;;#b101111
    (emit "sete   %dl")
    (emit "xorl   %eax, %eax")
    (emit "mov    %dl,  %al")
    (emit "shll   $~s,  %eax" bool_tag_width)
    (emit "orl    $~s,  %eax" bool_tag)
)

(define-primitive (null? si env expr)
    (emit-expr si env expr)
    (emit "cmpl   $63,   %eax") ;;#b00111111
    (emit "sete   %dl")
    (emit "xorl   %eax, %eax")
    (emit "mov    %dl,  %al")
    (emit "shll   $~s,  %eax" bool_tag_width)
    (emit "orl    $~s,  %eax" bool_tag)
)

(define-primitive (fxlognot si env expr)
    (emit-expr si env expr)
    (emit "sarl  $~s,  %eax" int_tag_width)
    (emit "notl  %eax")
    (emit "shll  $~s,  %eax" int_tag_width)
    (emit "orl   $~s,  %eax" int_tag)
)


;; native binary primitive 
(define-primitive (fx+ si env expr1 expr2) 
    (emit-expr si env expr1)
    (emit "sarl $~s,      %eax" int_tag_width)
    (emit "movl %eax,     ~s(%rsp)" si)
    (emit-expr (- si wordsize) env expr2)
    (emit "sarl $~s,      %eax" int_tag_width)
    (emit "addl ~s(%rsp), %eax" si)
    (emit "shll $~s,      %eax" int_tag_width)
    (emit "orl  $~s,      %eax" int_tag)
)

(define-primitive (fx- si env expr1 expr2) 
    (emit-expr si env expr2)
    (emit "sarl $~s,      %eax" int_tag_width)
    (emit "movl %eax,     ~s(%rsp)" si)
    (emit-expr (- si wordsize) env expr1)
    (emit "sarl $~s,      %eax" int_tag_width)
    (emit "subl ~s(%rsp), %eax" si)
    (emit "shll $~s,      %eax" int_tag_width)
    (emit "orl  $~s,      %eax" int_tag)
)

